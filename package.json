{
  "name": "tailwind-nextjs-starter-blog",
  "version": "2.1.0",
  "private": true,
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "start": "next dev --turbopack -p 3456",
    "dev": "cross-env INIT_CWD=$PWD next dev --turbopack -p 3456",
    "build": "cross-env INIT_CWD=$PWD next build --turbopack && cross-env NODE_OPTIONS='--experimental-json-modules' node ./scripts/postbuild.mjs",
    "serve": "next start",
    "analyze": "cross-env ANALYZE=true next build --turbopack",
    "lint": "eslint .",
    "postinstall": "contentlayer2 build"
  },
  "dependencies": {
    "@next/bundle-analyzer": "latest",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/typography": "^0.5.15",
    "autoprefixer": "latest",
    "contentlayer2": "latest",
    "esbuild": "^0.25.0",
    "github-slugger": "^2.0.0",
    "gray-matter": "^4.0.3",
    "image-size": "1.2.0",
    "next": "latest",
    "next-contentlayer2": "latest",
    "next-themes": "^0.4.4",
    "pliny": "latest",
    "postcss": "latest",
    "react": "latest",
    "react-dom": "latest",
    "reading-time": "1.5.0",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-citation": "^2.1.1",
    "rehype-katex": "^7.0.1",
    "rehype-preset-minify": "7.0.0",
    "rehype-prism-plus": "^2.0.0",
    "rehype-slug": "^6.0.0",
    "remark": "^15.0.1",
    "remark-gfm": "^4.0.0",
    "remark-math": "^6.0.0",
    "tailwindcss": "latest",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@svgr/webpack": "^8.0.1",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/mdx": "latest",
    "@types/react": "latest",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "cross-env": "^7.0.3",
    "eslint": "latest",
    "eslint-config-next": "latest",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.0.0",
    "lint-staged": "^13.0.0",
    "prettier": "latest",
    "prettier-plugin-tailwindcss": "latest",
    "typescript": "latest"
  },
  "resolutions": {
    "@opentelemetry/api": "1.4.1",
    "@opentelemetry/core": "1.13.0",
    "@opentelemetry/exporter-trace-otlp-grpc": "0.39.1",
    "@opentelemetry/resources": "1.13.0",
    "@opentelemetry/sdk-trace-base": "1.13.0",
    "@opentelemetry/sdk-trace-node": "1.13.0",
    "@opentelemetry/semantic-conventions": "1.13.0"
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "eslint --fix"
    ],
    "*.+(js|jsx|ts|tsx|json|css|md|mdx)": [
      "prettier --write"
    ]
  },
  "packageManager": "yarn@3.6.1"
}
